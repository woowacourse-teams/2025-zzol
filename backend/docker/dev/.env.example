# Docker Registry (e.g. ghcr.io/your-username)
REGISTRY=ghcr.io/your-username

# Image Tag (e.g. latest, v1.0.0)
IMAGE_TAG=latest

# JVM Memory Configuration
# Docker 메모리 제한: 2GB
# 권장 설정: -Xms1400m -Xmx1400m (Docker limit의 70%)
JAVA_OPTS=-Xms1400m -Xmx1400m

# Database Password
DB_PASSWORD=change_me

# Observability Configuration
TEMPO_URL=http://tempo:4318/v1/traces
TRACE_SAMPLING_PROBABILITY=1.0
